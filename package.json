{
  "name": "package.json",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "VITE_BUILD_DATE=$(date -u +\"%Y-%m-%d %H:%M UTC\") vite --host",
    "build": "run-p type-check \"build-only {@}\" -- && cp dist/index.html dist/404.html",
    "postbuild-only": "cp dist/index.html dist/404.html",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "build-and-publish": "VITE_BUILD_DATE=$(date -u +\"%Y-%m-%d %H:%M UTC\") run-p type-check \"build-only {@}\" -- && cp dist/index.html dist/404.html && git add dist && git commit -m \"Updating subtree\" && git subtree split --prefix dist -b gh-pages-temp && git push origin gh-pages-temp:gh-pages --force && git branch -D gh-pages-temp"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.8",
    "@mdi/js": "^7.4.47",
    "fast-xml-parser": "^5.3.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251119.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "~1.23.0",
    "eslint-plugin-vue": "~10.5.0",
    "express": "^5.2.1",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.23.0",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.0",
    "unplugin-fonts": "^1.4.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.56.0"
  }
}
